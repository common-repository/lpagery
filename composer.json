{
    "name": "niklas/lpagery",
    "type": "project",
    "authors": [
        {
            "name": "niklas",
            "email": "niklas@lpagery.io"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6",
        "10up/wp_mock": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.61",
        "phpstan/phpstan": "^1.11",
        "phpstan/extension-installer": "^1.4",
        "szepeviktor/phpstan-wordpress": "^1.3"
    },
  "require": {
    "ext-json": "*",
    "ext-zip": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "LPagery\\": "src/"
    }
  },
    "config": {
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
